emp = eval(input(" Enter Temperature outside .."))
 
if temp >= 1 and temp <= 20:
	print("extreme cold")
elif temp >= 21 and temp <= 30:
	print("cold")
elif temp >= 31 and temp <= 37:
	print("lukewarm")
elif temp >= 38 and temp <= 45:
	print("hot")
elif temp >= 45 and temp <= 50:
	print("boiling hot")
elif temp >= 50:
	print("dangerous")
else:
	print("invalid")
